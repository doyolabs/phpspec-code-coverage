formatter.name: pretty
suites:
    doyo_coverage:
        namespace: Doyo\PhpSpec\CodeCoverage
        psr4_prefix: Doyo\PhpSpec\CodeCoverage

extensions:
    Doyo\PhpSpec\CodeCoverage\Extension:
        whitelist:
            - src
        format:
            - php
            - html
        output:
            php: build/cov/phpspec.cov
            html: build/phpspec
